using UnityEngine;

public class Move : MonoBehaviour
{
    public int health = 5;
    public int level = 1;
    public float speed = 1.2f;

    void Start()
    {
        
    }

    // Update is called once per frame
    void Update()
    {
        Vector3 NewPos;
        NewPos = transform.position;
        NewPos.z += speed * Time.deltaTime;
        transform.position = NewPos;
    }
}
